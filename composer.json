{
	"name": "iwink/gitlab-webhook-bundle",
	"type": "symfony-bundle",
	"description": "A Symfony bundle to process GitLab webhooks.",
	"keywords": [
		"Symfony",
		"GitLab",
		"webhook"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "iWink",
			"email": "hallo@iwink.nl",
			"homepage": "https://www.iwink.nl/"
		}
	],
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"doctrine/annotations": "^1.11 | ^2.0",
		"symfony/config": "^5.0 | ^6.0",
		"symfony/dependency-injection": "^5.0 | ^6.0",
		"symfony/http-kernel": "^5.0 | ^6.0",
		"symfony/yaml": "^5.0 | ^6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.4",
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": { "Iwink\\GitLabWebhookBundle\\": "src/" }
	},
	"autoload-dev": {
		"psr-4": { "Iwink\\GitLabWebhookBundle\\Tests\\": "tests/" }
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	}
}
